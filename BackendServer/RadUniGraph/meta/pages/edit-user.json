{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "type": "close-popup"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Email",
                              "component": "Email",
                              "name": "EmailLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Email",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${user.Email}"
                            },
                            {
                              "type": "required-validator",
                              "name": "EmailRequiredValidator",
                              "component": "Email",
                              "text": "Email is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Password",
                              "component": "Password",
                              "name": "PasswordLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "Password",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${user.Password}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is User",
                              "component": "isUser",
                              "name": "isUserLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "isUser",
                              "style": {
                                "display": "block"
                              },
                              "value": "${user.isUser}"
                            },
                            {
                              "type": "required-validator",
                              "name": "isUserRequiredValidator",
                              "component": "isUser",
                              "text": "isUser is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Is Admin",
                              "component": "isAdmin",
                              "name": "isAdminLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "bool",
                              "type": "checkbox",
                              "name": "isAdmin",
                              "style": {
                                "display": "block"
                              },
                              "value": "${user.isAdmin}"
                            },
                            {
                              "type": "required-validator",
                              "name": "isAdminRequiredValidator",
                              "component": "isAdmin",
                              "text": "isAdmin is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Phone Number",
                              "component": "PhoneNumber",
                              "name": "PhoneNumberLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "type": "textbox",
                              "name": "PhoneNumber",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${user.PhoneNumber}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update User",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "UniSQL.updateUser",
                        "parameters": [
                          {
                            "name": "UserId",
                            "value": "${parameters.UserId}"
                          },
                          {
                            "name": "User",
                            "value": "${user}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${user}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "UniBlocksGraph.Models.UniSql.User",
                  "data": "${user}",
                  "visible": "${user != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "UniSQL.getUserByUserId",
        "parameters": [
          {
            "name": "UserId",
            "value": "${parameters.UserId}"
          }
        ],
        "then": [
          {
            "name": "user",
            "type": "set",
            "value": "${result}",
            "valueType": "UniBlocksGraph.Models.UniSql.User"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit User",
  "layout": "Main",
  "access": [
    "Authenticated"
  ]
}